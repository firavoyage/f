{
  "manifest_version": 3,
  "name": "Intention",
  "version": "1.0",
  "description": "A calm new tab override that observes tabs and omnibox.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "omnibox": {
    "keyword": "intent"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "intention@example.com",
      "strict_min_version": "109.0"
    }
  }
}
